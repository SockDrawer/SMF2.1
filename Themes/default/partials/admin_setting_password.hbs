		{{> admin_setting_dt_container config_var=config_var}}
		<dd{{#if config_var.force_div_id}} id="{{config_var.force_div_id}}_dd"{{/if}}>
			{{{config_var.preinput}}}
			<input type="checkbox" {{{config_var.javascript}}}{{#if config_var.disabled}} disabled{{/if}} name="{{config_var.name}}" id="{{config_var.name}}"{{#if config_var.value}} checked{{/if}} value="1">
			<input type="password" {{{config_var.javascript}}}{{#if config_var.disabled}} disabled{{/if}} name="{{config_var.name}}[0]" {{#if (gt config_var.size 0)}} size="{{config_var.size}}"{{/if}} value="*#fakepass#*" onfocus="this.value = ''; this.form.{{config_var.name}}.disabled = false;"><br>
			<input type="password" disabled id="{{config_var.name}}" name="{{config_var.name}}[1]"{{#if (gt config_var.size 0)}} size="{{config_var.size}}"{{/if}}>
			{{{config_var.postinput}}}
		</dd>